/*progress bar/wizar css*/


.wizard{
    width:100%;
    box-sizing:border-box;
    border:#2785c3 1px solid;
    margin-bottom:20px;
    background:#2785c3;
    }
  .wizard a:hover{
      text-decoration:none;
      color:blue;
    }  

.wizard a {
    padding:7px 0 6px 0;
    background:#fff; 
    position:relative; display:inline-block; 
    width:33.1% ;
    box-sizing:border-box; 
    text-align:center;

   
}
.wizard a:last-child {
    margin-right:0;
    padding-right:0;
}
.wizard a:before,
.wizard a:after {
         width:0px;
     height:0px; 
     border-top: 14px inset transparent;
     border-bottom: 15px inset transparent;
     position: absolute;
     content: ""; }




.wizard a:before {

      
     border-left: 15px solid #2785c3; 
    top: 0; left:-1px;
     }
.wizard a:after {
  
     
    border-left: 15px solid #fff; 
    top: 0; right: -15px;
    z-index:2;
 }
 
 
.wizard a:first-child:before {border:none;}
.wizard a:last-child:after {border:none; }

/*.wizard a:first-child {-moz-border-radius: 4px 0 0 4px; -webkit-border-radius: 4px 0 0 4px; 
border-radius:   4px 0 0 4px;}
.wizard a:last-child {-moz-border-radius: 0 4px 4px 0; -webkit-border-radius: 0 4px 4px 0;
 border-radius:   0 4px 4px 0;}*/

/*.wizard .badge {margin:0 5px 0 18px; position:relative; top:-1px;}
.wizard a:first-child .badge {margin-left:0;}*/

.wizard .current {background:#2785c3; color:#fff;}
.wizard .current:after {border-left-color:#2785c3;}
.wizard .current:before {border-left-color:#2785c3;}


/*end of progress bar css*/